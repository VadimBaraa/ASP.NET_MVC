@model List<ASP.NET_MVC.Models.LogEntry>

@{
    ViewData["Title"] = "Log";
}

<h1>Log</h1>

<table class="table">
    <thead>
        <tr>
            <th>Timestamp</th>
            <th>Message</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var log in Model)
        {
            <tr>
                <td>@log.Timestamp</td>
                <td>@log.Message</td>
            </tr>
        }
    </tbody>
</table>
